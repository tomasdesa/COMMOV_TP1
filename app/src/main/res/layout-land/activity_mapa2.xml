<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/drawerLayout">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <fragment
        android:id="@+id/map"
        class="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="-1dp" />

    <ImageView
        android:id="@+id/imgmenu"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="652dp"
        android:layout_marginRight="652dp"
        android:src="@drawable/ic_baseline_menu_24" />

    <Button
        android:id="@+id/btn500"
        android:layout_width="74dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        android:layout_marginBottom="174dp"
        android:clickable="true"
        android:gravity="end"
        android:text="@string/_500m"
        android:textSize="14sp" />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/fabdistancias"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="3dp"
        android:layout_marginBottom="122dp"
        android:clickable="true"
        android:gravity="end"
        android:text="@string/Distancias"
        android:textSize="14sp"
        app:icon="@drawable/ic_baseline_linear_scale_24" />

    <Button
        android:id="@+id/btn1000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="3dp"
        android:layout_marginBottom="228dp"
        android:clickable="true"
        android:gravity="end"
        android:text="@string/_1000m"
        android:textSize="14sp" />

    <Button
        android:id="@+id/btn1500"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="3dp"
        android:layout_marginBottom="281dp"
        android:clickable="true"
        android:gravity="end"
        android:text="@string/_1500m" />

    <Button
        android:id="@+id/Markersdefault"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="244dp"
        android:layout_marginRight="244dp"
        android:layout_marginBottom="66dp"
        android:clickable="true"
        android:gravity="end"
        android:text="@string/markerdefault" />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/fabtipo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:layout_marginBottom="71dp"
        android:clickable="true"
        android:gravity="end"
        android:text="@string/TipoProblema"
        app:icon="@drawable/ic_baseline_report_problem_24" />

    <Button
        android:id="@+id/estradafechada"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="100dp"
        android:layout_marginRight="100dp"
        android:layout_marginBottom="19dp"
        android:clickable="true"
        android:gravity="end"
        android:text="@string/estradaf" />

    <Button
        android:id="@+id/acidente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="458dp"
        android:layout_marginRight="458dp"
        android:layout_marginBottom="20dp"
        android:clickable="true"
        android:gravity="end"
        android:text="@string/Acidente" />

    <Button
        android:id="@+id/buraco"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="567dp"
        android:layout_marginRight="567dp"
        android:layout_marginBottom="20dp"
        android:clickable="true"
        android:gravity="end"
        android:text="@string/Buraco" />

    <Button
        android:id="@+id/obras"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="155dp"
        android:layout_marginRight="155dp"
        android:layout_marginBottom="67dp"
        android:clickable="true"
        android:gravity="end"
        android:text="@string/obras" />

    <Button
        android:id="@+id/engarrafamento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="279dp"
        android:layout_marginRight="279dp"
        android:layout_marginBottom="19dp"
        android:clickable="true"
        android:gravity="end"
        android:text="@string/engarrafamento" />


</RelativeLayout>
    <include layout="@layout/nav_drawer"></include>

</androidx.drawerlayout.widget.DrawerLayout>